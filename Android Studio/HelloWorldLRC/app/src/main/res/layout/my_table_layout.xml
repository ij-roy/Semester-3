        <Button
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:onClick="loadMenuLayout"
            android:text="Menu" />
    </TableRow>
</TableLayout>